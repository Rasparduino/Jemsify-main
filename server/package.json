{
  "name": "spotify-clone-server",
  "version": "1.0.0",
  "description": "Backend server for Spotify clone with yt-dlp integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "spotify",
    "music",
    "streaming",
    "yt-dlp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "multer": "^1.4.4-lts.1",
    "node-cron": "^3.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "nodemonConfig": {
    "watch": [
      "routes/",
      "services/",
      "server.js"
    ],
    "ext": "js",
    "ignore": [
      "downloads/",
      "*.json"
    ],
    "verbose": true,
    "delay": "1000"
  }
}